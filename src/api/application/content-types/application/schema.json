{
  "kind": "collectionType",
  "collectionName": "applications",
  "info": {
    "singularName": "application",
    "pluralName": "applications",
    "displayName": "Application",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "isDeleted": {
      "type": "boolean",
      "default": false
    },
    "job": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::job.job",
      "inversedBy": "applications"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "Under Review",
        "Rejected ",
        "Selected"
      ],
      "default": "Under Review",
      "required": false
    },
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true,
      "unique": false
    },
    "phoneNo": {
      "type": "biginteger",
      "unique": false,
      "required": true
    }
  }
}
